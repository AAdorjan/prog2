import numpy as np        
import math               
                          
                          
l=np.random.randint(0,100,
setA=set(l)               
                          
minv=np.inf               
maxv=-np.inf              
                          
for i in setA:            
    if i> maxv:           
        maxv=i            
    if i<minv:            
        minv=i            
print(setA)               
