r=int(input('sor: '))       
c=int(input('sor: '))       
                            
M=np.zeros((r,c))           
for i in range(0,r):        
    for j in range(0,c):    
        M[i,j]=(i+1)*(j+1)  
                            
print(M)                    
                                                    
